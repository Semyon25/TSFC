        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TSFC"
        mc:Ignorable="d"
        DataContext="{Binding LogicTableViewModel, Source={StaticResource Locator}}"
        Title="Taблица истинности" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="grid" Grid.Row="1" ItemsSource="{Binding LogicTable}" />
    </Grid>
</Window>
